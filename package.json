{
  "name": "farmr",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://demo.getfarmr.org",
  "dependencies": {
    "firebase": "^8.2.1",
    "immutability-helper": "^3.1.1",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-firestore": "^1.0.1",
    "react-ga": "^3.3.0",
    "react-instantsearch-dom": "^6.8.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-transition-group": "^4.4.1",
    "slugify": "^1.4.6",
    "styled-components": "^5.2.1"
  },
  "scripts": {
    "": "How these scripts work: https://medium.com/@tacomanator/environments-with-create-react-app-7b645312c09d",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dev": "  sh -ac '. ./.env.dev;   firebase use dev   && react-scripts build && firebase deploy'",
    "stage": "sh -ac '. ./.env.stage; firebase use stage && react-scripts build && firebase deploy'",
    "live": " sh -ac '. ./.env.live;  firebase use live  && react-scripts build && firebase deploy'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
